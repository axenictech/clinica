<div id="content-header">
  <h1>Subjects</h1>
  <h3>Home</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
 <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">
<div class="box">

<div class="text-input-bg">
	<%= form_for :batch, url:{action:'select'},method: :get,remote:true,html:{id: "subject_form"}  do |f| %>
	<%= f.label :select_batch %>
  <%= f.select :id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name,b.id]}, {prompt:true},{onchange:"$('#subject_form').submit();"}%>
<% end %>
</div><br /><br /><br/>
 
<div id="subject_div"></div>
 <div id="myModal" class="modal fade">
 </div>
 </div></div>