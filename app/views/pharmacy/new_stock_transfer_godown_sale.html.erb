    <div class="box-content box-double-padding">
  
        <fieldset>
          <div class="row">
            <div class="form-group">
              <label class="col-sm-2 control-label">
                Transfer No:</label>
                <div class="col-sm-6"><%= o.text_field :transfer_no,
                    onfocus:"$(this).popover('show');" ,
                     class: "form-control input-group",placeholder: "Transfer No",
                      required: true,
                    
                      :"data-placement"=>"right",
                      :"data-toggle"=>"popover",:"data-trigger"=>"focus"
                      %>  
                  </div>     
                </div>
          </div>
          <br>
           <div class="row">
            <div class="form-group">
              <label class="col-sm-2 control-label">
                Transfer Date:</label>
                <div class="col-sm-6"><%= o.date_field :transfer_date,:value => Time.now.strftime('%Y-%m-%d'),
                    onfocus:"$(this).popover('show');" ,
                     class: "form-control input-group",
                      required: true,
                    
                      :"data-placement"=>"right",
                      :"data-toggle"=>"popover",:"data-trigger"=>"focus"
                      %>  
                  </div>     
                </div>
          </div>
