<div id="content-header">
   <h1>Online Exam</h1>
  <h3>Viw online Exam</h3>
  <div id="app-back-button">
  <%= link_to image_tag("buttons/back.png",:border => 0), 'setting/course_batch' %>
  </div>
 <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">
    <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>
<div class="box">
   <div class="text-input-bg">
	
		<%= form_for :batch, url:{action:'select'},method: :get,remote:true,html:{id: "batch_form"} do |f| %>
				
		<%= f.label :select_batch%>
		<%= f.select :id, @batches.collect {|c| [c.name,c.id]},
              {prompt:true},{onchange:"$('#batch_form').submit();"}%>
		<% end %>
	</div>
			
</div>

<div id="exam_div"></div>
			
