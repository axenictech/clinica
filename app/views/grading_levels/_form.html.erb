<div class="modal-body">
        <ul class="errors"></ul>
<%= form_for [@batch,@grading_level1],remote: true do |f|%>
 <table class="table" style="width:85%">
        <tr>
            <td class="col-2"><%= f.label :name , class: "control-label"%></td>
            <td><%=f.text_field :name ,:style =>"width: 250px", class: "form-control"%></td>
        </tr>
        
        <tr>
            <td class="col-2"><%= f.label :min_score , class: "control-label"%></td>
            <td><%=f.text_field :min_score,:style =>"width: 250px", class: "form-control" %></td>
        </tr>
        
        <tr>
            <td class="col-2"><%= f.label :description, class: "control-label"%></td>
            <td><%=f.text_field :description ,:style =>"width: 250px", class: "form-control"%></td>
        </tr>
        <tr>
        <td colspan="2"><%= f.submit 'Submit', class: "btn btn-primary"%>
        <%= link_to 'cancel', '#', class: "btn btn-default",data:{dismiss:"modal"}%></td>
        </tr>
    </table>
<% end %>
</div>

