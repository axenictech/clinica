                    onfocus:"$(this).popover('show');" ,
                     class: "form-control input-group",placeholder: "H.R.No",
                      required:false,
                      :"data-content"=>"Special characters not allowed",
                      :"data-placement"=>"right",
                      :"data-toggle"=>"popover",:"data-trigger"=>"focus"
                      %>  
                  </div>     
                </div>
          </div>
          <br>
          <div class="row">
            <div class="form-group">
              <label class="col-sm-2 control-label">OPD NO:</label>
                <div class="col-sm-6"><%= c.text_field :OPD_no,
                    onfocus:"$(this).popover('show');" ,
                     class: "form-control input-group",placeholder: "Specailized",
                      required:false,
                      :"data-content"=>"Special characters not allowed",
                      :"data-placement"=>"right",
                      :"data-toggle"=>"popover",:"data-trigger"=>"focus"
                      %>  
