<div class="page-header page-header-with-buttons">
  <h1 class="pull-left">
    <i class="icon-book"></i>
    <span>OutDoor(Old Patient) </span>
  </h1>
<div class="pull-right">
  <ol class="breadcrumb">
      <li class="active">
        <i class="fa fa-gears"></i> 
        <a href=""></a>
      </li>
  </ol>
</div>
</div>

<div id="flash"><%= render 'layouts/flash_messages'%></div>

<%=form_for @old_patient,url:{action:'old_registration_create'}  do |c| %>

