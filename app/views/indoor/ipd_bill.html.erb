<div id="content-header">

  <h1>Bill</h1>
  <h3>view bill</h3>

  <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="page-yield">
 <div class="box">
<div class="text-input-bg">

<table class="table">
<tr>
<th>Check</th>
<th>Name</th>
<th>Age</th>
<th>Address</th>
<th>Total Amount</th>
<th></th>
</tr>
<%@bill.each do |b|%>
<tr>
 <td><%= check_box_tag "bills[]", b.id,false,class: 'check_box'%></td>
<td><%=b.name%></td>
<td><%=b.age%></td>
<td><%=b.address%></td>
<td><%=b.total_amt%></td>
</tr>
<%end%>
</table>
</div>
</div>
</div>