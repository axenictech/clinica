<div class="modal-dialog">
<div class="modal-content">
<div class="modal-header">
<h3>Edit Fee Discount</h3>
</div>

<div class="modal-body">
<ul class="errors"></ul>
    <%= form_for @discount,url:{action:'update_fee_discount',batch_id:@batch.id,master_id:@master_category.id,id:@discount.id},method: :patch,remote: true do |f|%>

        <div class="row">
            <div class="col-md-1"></div>
            <div class="col-md-3"><%= f.label :name %></div>
            <div class="col-md-3"><%= f.text_field :name,:style =>"width: 250px",class: "form-control"%></div>
        </div><br/>
        <div class="row">
        <div class="col-md-1"></div>
            <div class="col-md-3"><%= f.label :discount%></div>
            <div class="col-md-3"><%= f.text_field :discount,:style =>"width: 250px",class: "form-control"%></div>
        </div><br/>
        <div class="row">
        <div class="col-md-2"></div>
            <div class="col-md-3"><%= f.submit 'Update', class: "btn btn-primary"%></div>
            <div class="col-md-3"><%= link_to 'cancel', '#', class: "btn btn-default",data:{dismiss:"modal"}%></div>
        </div>
    <%end%>
</div>    
</div>
</div>
