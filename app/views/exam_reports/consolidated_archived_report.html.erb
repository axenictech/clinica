<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Archived Student Consolidated Report</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="inner-tab-menu">
  <ul>
    <li><%= link_to 'Pdf report',exam_reports_archived_students_consolidated_report_path(batch_id:@batch.id,format:"pdf")%></li>
  </ul>
</div>
<div id="page-yield">
  <div class="box">
  <div class="bread_crumb">
  <%= link_to 'Exams',  '/exams'  %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    <%= link_to 'Archived student report', '/exam_reports/archived_student_report' %> &raquo;
    Consolidated Report
  </div></div>
<%= form_for :exam_group_option, :url=>{:action=>'exam_group_wise_report'},html:{id: 'form_exam_group'},remote:true,method: :get do |f| %>
   <div class="label-field-pair">
  <%= f.label :select_exam_group %>
      <div class="text-input-bg">
      <%= f.select :id, @exam_groups.collect {|e| [e.name,e.id]},{prompt:true},{onchange:"$('#form_exam_group').submit();"}%>
      </div></div>
  <% end %>
  <br/><br/>
      <div id="consolidated_report"></div>
  </div>
</div>