<div id="content-header">

  <h1>Time table</h1>
  <h3>Teacher Timetable</h3>
  <div id="app-back-button">
      <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
  <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

 <div class="box">
<div class="text-input-bg">
<%= form_for :time,url:{action:'teacher_time_table_display'},method: :get,remote:true,html:{id: "time_form"} do |f| %>
<%= f.label :select_a_time %>
<%= f.select :id, @timetables.collect {|t| [t.start_date.strftime("%d %b %Y")+"-"+t.end_date.strftime("%d %b %Y"),t.id]},{prompt:true},{onchange:"$('#time_form').submit();"} %>  
<%end%>
</div><br/><br/><br/>


<div id="time_div"></div>
</div></div>
