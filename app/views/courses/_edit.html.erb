 <div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header"  >
            <button aria-hidden="true" class="close" data-dismiss="modal" type="button">Ã—</button>
            <h4 class="modal-title">Edit Course</h4>
        </div>
    <div class="modal-body">
     <ul class="errors"></ul>
    <%= form_for @course, remote:true do |c| %>
    <div class="form-group">
        <%=c.label :course_name %>
        <%=c.text_field :course_name , class: "form-control"%>
    </div>
    <div class="form-group">
        <%=c.label :section_name %>
        <%=c.text_field :section_name,class: "form-control" %>
    </div>
    <div class="form-group">
        <%=c.label :code %>
        <%=c.text_field :code , class: "form-control"%>
    </div>
    <div class="modal-footer">
        <%= c.submit 'update', class: "btn btn-primary pull-left "%>
    </div>
<% end %>
</div>
</div>
   

 


 