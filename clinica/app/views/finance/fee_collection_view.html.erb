<div id="content-header">
  <h1>Finance Collection Dates</h1>
  <h3>View fee collection dates</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">
<div id="box">
  <div class="label-field-pair">
  <%= label_tag :select_batch %>
      <div class="text-input-bg">
      <%= select :batch,:id, Batch.all.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name,b.id]},{prompt:true},{onchange:"var a={id: $('#batch_id').val()};$.get('view_fee_collection',a,function(response){});"}%>
      </div></div><br/><br/>
</div>
<div id="list"></div>
<div id="modal" class="modal fade"></div>
</div>