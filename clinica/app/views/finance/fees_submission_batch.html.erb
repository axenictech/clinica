<div id="content-header">
  <h1>Fees Submission</h1>
  <h3>Student fees submission</h3>
    <div id="app-back-button">
  <%= link_to image_tag("buttons/back.png",:border => 0), :back %>
  </div>
  <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">
<div id="box">
 <div class="label-field-pair">
  <%= label_tag :select_batch %>
      <div class="text-input-bg">
      <%= select :batch,:id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name,b.id]},{prompt:true},{onchange:"var a={id: $('#batch_id').val()};$.get('fee_collection_date',a,function(response){});"}%>
      </div></div><br/>
<div id="collection"><%= render 'fee_collection_date'%></div>
</div><br/><br/>
<div id="fees_details"></div>
</div>