<div id="content-header">
  <h1>Incomes</h1>
  <h3>View Income</h3>
    <div id="app-back-button">
  <%= link_to image_tag("buttons/back.png",:border => 0), :back %>
  </div>
  <div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">

<%= form_for :income, url:{action: 'income_list'} do |f| %>
<table class="table" style="width:500px" align="center">
<tbody>
		<tr>
			<td class="col-2"><%= f.label :select_start_date%></td>
			<td class="col-2"><%= f.date_field :start_date,:style =>"width: 200px", class: "form-control"%></td>
		</tr>
		<tr>
			<td class="col-2"><%= f.label :select_end_date%></td>
			<td class="col-2"><%= f.date_field :end_date,:style =>"width: 200px", class: "form-control"%></td>
		</tr>
		<tr>
			<td colspan="2"><%= f.submit 'View',class: "btn btn-default"%></td>
		</tr>
</tbody>
</table>
<%end%>
</div>
