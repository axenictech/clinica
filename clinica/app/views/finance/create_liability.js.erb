$("ul.errors").html("")
<% if @liability.errors.any? %>
		<% @liability.errors.full_messages.each do |message| %>
		$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
		<% end %>
<% else %>
  $("#Modal").modal("hide")
  $('#flash').html("<%= j render 'layouts/flash_messages'%>") 
<% end %>