<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Student Ranking Per Attendance</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="page-yield">
  <div class="box">

  <div class="bread_crumb">
    <%= link_to 'Exams', '/exams' %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    Student Ranking Per Attendance
  </div><br/><br/>

 
<%= form_for :rank_report, :url=>{:action=>'generate_student_ranking_report3'},method: :get do |f|%>
  
 <div class="label-field-pair">
  <%= f.label :select_batch %>
      <div class="text-input-bg">
      <%= f.select :batch_id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name,b.id]},{prompt:true}%>
      </div></div>

 <div class="label-field-pair">
       <%= f.label :select_start_date %>
      <div class="text-input-bg">
      <%= f.date_field :start_date,:style =>"height: 30px" %>
      </div></div>

 <div class="label-field-pair">
       <%= f.label :select_end_date %>
      <div class="text-input-bg">
      <%= f.date_field :end_date,:style =>"height: 30px"%>
      </div></div><br/>
<div align="center"><%= f.submit 'View', class: 'btn btn-default' %></div>
  <% end %>
  </div>
</div> 