<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Grouped exam reports</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="inner-tab-menu">
  <ul>
      <li><%= link_to 'Pdf Report',exam_reports_grouped_exam_students_report_path(batch_id:@batch.id,format:"pdf")%></li>
  </ul>
</div>
<div id="page-yield">
    <div id="box">
  <div class="bread_crumb">
    <%= link_to 'Exams',  '/exams'  %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    <%= link_to 'Grouped exam report', '/exam_reports/grouped_exam_report' %> &raquo;
    Generated report
  </div></div>
  <h3 align="center"><%= @batch.name %> - <%= @batch.course.course_name %></h3>
  <div id="student_list" align="left" style="margin-left:50px">
    <%= render 'grouped_exam_student_list' %>
  </div>
  <div id="grouped_exam_report" align="right">
    <%= render 'grouped_exam_report' %>
  </div>
  </div>