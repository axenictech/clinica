<% unless @batch.students.empty?  %>
  <div class="list_name">
<h3>Students in batch</h3>
  </div><br/>
  <ul class="student_list">
    <% @batch.students.each do |s| %>
      <li class="student_names">
      <%=link_to s.first_name+" "+s.last_name,exam_reports_student_exam_report_path(@exam_group,s),remote:true%>
      </li>
    <% end %>
  </ul>
<% else %>
  <h4>Student Not Found</h4>
<% end %>