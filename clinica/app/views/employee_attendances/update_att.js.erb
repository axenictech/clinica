$("ul.errors2").html("")
<% if @attendance.errors.any? %>
		<%  @attendance.errors.full_messages.each do |message| %>
		$("ul.errors2").append($("<li />").html("<%= message.html_safe %>"))
		<% end %>
<% else %>
$('#employee_div').html("<%= j render 'employee'%>")
$("#myModal").modal("hide")
<%end%>
$('#flash').html("<%= j render 'layouts/flash_messages'%>")