<div id="content-header">
  <h1>Manage Users</h1>
  <h3>Details about users</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0),:back %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>

<div id="inner-tab-menu">
  <ul>
    <li><%= link_to "View all", users_view_all_path %></li>
    <li><%= link_to "Add new", new_user_path %></li>
  </ul>
</div>

<div id="page-yield">
 <% unless flash[:user_delete].nil? %>
    <div align="center" class="alert alert-success" style="width:50%"> <%= flash[:user_delete] %> </div>
  <% end %>
   <div align="center">
    <%= form_for :user, url:{action:'search'}, method: :get,remote:true,html:{id: "user_form"} do |f|%>
       <b> Search User</b>
          <%= text_field_tag :search,"",{oninput:"$('#user_form').submit();"}%>
        <% end %>
  </div>
  <div id="user_search"></div>
</div>