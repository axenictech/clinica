<div class="modal-body">
        <ul class="errors"></ul>
<%= form_for [@course,@rank_lev1],url:{:action=>'createrank',course_id: @course.id},method: :post,remote: true do |f|%>
<table class="table" style="width:85%">
<tr>
    <td class="col-2"><%= f.label :name , class: "control-label"%></td>
    <td><%=f.text_field :name ,:style =>"width: 250px", class: "form-control"%></td>
</tr>
        
 <tr>
    <td class="col-2"><%= f.label :marks , class: "control-label"%></td>
    <td><%=f.text_field :marks ,:style =>"width: 250px", class: "form-control"%></td>
  </tr>

  <tr>
     <td class="col-2"><%= f.label :marks_limit_Type , class: "control-label"%></td>
      
        <td><%=f.select :marks_limit_type, ["Upper", "Lower", "Exact"],:style =>"width: 250px", class: "form-control" %></td>

  </tr>

  <tr>
      <td class="col-2"><%= f.label :Number_Of_Subjects , class: "control-label"%></td>
      <td><%=f.text_field :subject_count ,:style =>"width: 250px", class: "form-control"%></td>
  </tr>

  <tr>
     <td class="col-2"><%= f.label :Subject_limit_Type , class: "control-label"%></td>
        <td><%=f.select :subject_limit_type, ["Upper", "Lower", "Exact"],:style =>"width: 250px", class: "form-control" %></td>
  
  </tr>
   <tr>

         <td class="col-2"><%= f.label :Consider_full_course_duration? , class: "control-label"%></td>
            
            <td><%= f.check_box :full_course, :checked => false %> </td>
        </tr>
   <tr>
        <td colspan="2"><%= f.submit 'Submit', class: "btn btn-primary"%>
        <%= link_to 'cancel', '#', class: "btn btn-default",data:{dismiss:"modal"}%></td>
        </tr>
    </table>
<% end %>
</div>