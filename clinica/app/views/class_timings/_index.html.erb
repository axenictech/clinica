 <% @class_timings.order('start_time ASC').each do |c| %>
        
            <td class="col-2"><%= c.name %></td>
            <td class="col-2"><%= c.start_time.strftime("%I:%M %p")%></td>
            <td class="col-2"><%= c.end_time.strftime("%I:%M %p")%></td>
            <td><%=link_to 'Edit',edit_batch_class_timing_path(c.batch,c),remote: true%></td>
            <td><%= link_to 'Delete',[c.batch,c],remote: true,
                    method: :delete, data: { confirm: "Are you sure to delete #{c.name} Class Timing?" } %>
                    </td>
            
        </tr>   
            
      <% end %>
