<div id="content-header">
  <h1>Time table</h1>
  <h3>Create Timetable</h3>
  <div id="app-back-button">
      <%= link_to image_tag("buttons/back.png",:border => 0), :back  %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="page-yield">
 <div class="box">
<div class="text-input-bg">
<%= form_for :batch, url:{action:'select',format:@time},method: :get,remote:true,html:{id: "batch_form"} do |f| %>
<%= f.label :select_batch %>
<%= f.select :id, @batches.collect {|b| [b.course.course_name+" "+b.course.section_name+" - "+b.name, b.id]},{prompt:true},{onchange:"$('#batch_form').submit();"} %>  
<%end%>
</div>
<div id="batch_div"></div><br/><br/><br/>
</div>
</div>